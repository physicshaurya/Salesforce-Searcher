{
    "manifest_version": 3,
    "name": "Salesforce Searcher",
    "version": "1.1",
    "description": "Search for information based on record ID or Object Name",
    "permissions": [
        "tabs"
      ],
    "icons": {
        "128": "images/iconNoBG128png.png"
      },
    "action": {
        "default_icon": {             
          "128": "images/IconLogoSearch.png"   
        },
        "default_title": "Salesforce Searcher",   
        "default_popup": "popup.html"  
      }
  }
  